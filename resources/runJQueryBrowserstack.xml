<?xml version="1.0" encoding="UTF-8"?>
<suite name="JQuery" parallel = "classes" thread-count = "3">

 <parameter name = "url" value="https://www.jqueryscript.net/demo/CRUD-Data-Grid-Plugin-jQuery-Quickgrid/"/>

	<listeners>
			<listener class-name="commons.MethodListener"/>
			<listener class-name="reportConfig.ExtentTestListener"/>
			<listener class-name="reportConfig.AllureTestListener"/>
			<listener class-name="org.uncommons.reportng.HTMLReporter" />
			<listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
	</listeners>
	
	<test name="Run on chrome">
		<parameter name="browser" value="chrome" />
		<parameter name="osName" value="Windows" />
		<parameter name="osVersion" value="10" />
		<classes>
			<class name="com.jquery.cloud.Level_22_Live_Config_III_Browserstack" />
		</classes>
	</test>
	
	<test name="Run on Firefox">
		<parameter name="browser" value="firefox" />
		<parameter name="osName" value="OS X" />
		<parameter name="osVersion" value="Big Sur" />
		<classes>
			<class name="com.jquery.cloud.Level_22_Live_Config_III_Browserstack" />
		</classes>
	</test>

</suite>
