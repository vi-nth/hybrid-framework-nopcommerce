<?xml version="1.0" encoding="UTF-8"?>
<suite name="JQuery" parallel = "classes" thread-count = "3">

 <parameter name = "envName" value="browserStack"/>
 <parameter name = "severName" value="dev"/>
 
	<listeners>
			<listener class-name="commons.MethodListener"/>
			<listener class-name="reportConfig.ExtentTestListener"/>
			<listener class-name="reportConfig.AllureTestListener"/>
			<listener class-name="org.uncommons.reportng.HTMLReporter" />
			<listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
	</listeners>
	
	<test name="Run on firefox">
		<parameter name="browser" value="firefox" />
		<parameter name="osName" value="Windows" />
		<classes>
			<class name="com.jquery.cloud.Level_22_Live_Config_VII_All" />
		</classes>
	</test>
	
	<test name="Run on safari">
		<parameter name="browser" value="safari" />
		<parameter name="osName" value="MacOS 11.0" />
		<parameter name="osVersion" value="Big Sur" />
		<classes>
			<class name="com.jquery.cloud.Level_22_Live_Config_VII_All" />
		</classes>
	</test>

</suite>
